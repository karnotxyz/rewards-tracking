{
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run --watch --allow-read --allow-net --allow-write --allow-ffi --allow-env src/backend/main.ts"
  },
  "imports": {
    "@nestjs/common": "npm:@nestjs/common@^10.4.15",
    "@nestjs/core": "npm:@nestjs/core@^10.4.15",
    "@nestjs/platform-express": "npm:@nestjs/platform-express@^10.4.15",
    "@prisma/client": "npm:@prisma/client@6.1.0",
    "@std/assert": "jsr:@std/assert@1",
    "generated/": "./node_modules/generated/",
    "prisma/": "./prisma/"
  },
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
